<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TypeConverterApproachActivity">

    <Button
        android:id="@+id/btnCreateStudentWithJson"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Create Student w/ Json"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnGetStudentsWithJson"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Get Students w/ Json"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnCreateStudentWithJson" />

</androidx.constraintlayout.widget.ConstraintLayout>